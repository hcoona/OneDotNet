<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>$(DefaultLibraryTargetFrameworks)</TargetFrameworks>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Google.Protobuf" Version="3.15.0" />
    <PackageReference Include="Grpc" Version="2.25.0" />
    <PackageReference Include="Grpc.Tools" Version="2.25.0" PrivateAssets="All" />
  </ItemGroup>

  <ItemGroup>
    <Protobuf Include="../proto/bjdire/v1/authentication.proto" Link="authentication_v1.proto" />
    <Protobuf Include="../proto/bjdire/v1/device.proto" Link="device_v1.proto" />
    <Protobuf Include="../proto/bjdire/v2/service.proto" Link="service_v2.proto" />
  </ItemGroup>

</Project>
